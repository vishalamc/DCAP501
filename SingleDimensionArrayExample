import java.util.*;
class array1DExample
{
	

	public static void main(String []args)
	{
		int []num=new int[5];
		int i, sum=0;
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter 5 elements");

		for(i=0;i<5;i++)
		{
			num[i]=sc.nextInt();
		}
		for(i=0;i<5;i++)
		{
		sum=sum+num[i];
		}

		System.out.println("sum"+sum);
		

	}

}
